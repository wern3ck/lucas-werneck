CREATE DATABASE produtos;
USE produtos;
DROP TABLE IF EXISTS estoque;
CREATE TABLE estoque(
id_estoque INT PRIMARY KEY ,
nome VARCHAR(50) NOT NULL,
id_produtos INT NOT NULL,
id_OBJETO INT NOT NULL
);CREATE DATABASE produtos;

INSERT INTO estoque(id_estoque, nome, id_produtos, id_OBJETO)
VALUES
(10,'deposito',20, 100);
SELECT * FROM estoque;
DESC estoque;

DROP TABLE IF EXISTS produtos;
CREATE TABLE produtos(
id_produtos INT PRIMARY KEY,
nome VARCHAR(50) NOT NULL,
preco FLOAT(10) NOT NULL,
categoria VARCHAR(50) NOT NULL,
apedrejos VARCHAR(50) NOT NULL
);
insert into produtos(id_produtos, nome, preco, categoria, apedrejos)
VALUES
('1','lucas',25, 'objeto','camisa'),
('3','werneck',20,'esporte','bola'),
('2','vieira',15,'objeto','faca');
UPDATE produtos SET categoria = 'objeto' WHERE id = 2;
SELECT * FROM produtos;
DESC produtos;
